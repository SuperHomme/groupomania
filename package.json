      "url": "https://github.com/superhomme/groupomania/issues"
    },
    "homepage": "https://github.com/superhomme/groupomania#readme",
    "devDependencies": {
      "autoprefixer": "^9.8.0",
      "postcss": "^7.0.31",
      "postcss-cli": "^7.1.1",
      "sass": "^1.26.5"
    }
  }